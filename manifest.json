  {
    "manifest_version": 2,
    "name": "ChromeYe",
    "description": "This extension creates a Kanye West themed new tab page that includes a search and weather widget.",
    "version": "0.0.1",

    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "icons": {"128": "assets/icon128.jpg", "48": "assets/icon48.jpg"},
    "permissions": [
      "geolocation",
      "storage"
    ],
    "content_scripts": [ {
        "matches": ["*://*/*"],
        "css": ["assets/application.css"],
        "js": ["lib/bundle.js"]
      } ]
  }
